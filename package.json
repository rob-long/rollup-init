{
  "name": "@rob-long/rollup-init",
  "version": "0.0.1",
  "description": "Pure rollup starter project with typescript, jest, eslint, prettier, and typedoc",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "rollup -c",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "docs": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rob-long/rollup-init.git"
  },
  "homepage": "https://rob-long.github.io/rollup-init/modules.html",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [],
  "author": "rob-long <roblong@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "eslint": "^9.4.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.4",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  }
}
